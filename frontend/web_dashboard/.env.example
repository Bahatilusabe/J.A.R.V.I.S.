# Application
VITE_APP_NAME=J.A.R.V.I.S. Web Dashboard
VITE_APP_VERSION=2.1.0
VITE_APP_ENVIRONMENT=development

# Backend API
# Note: Backend runs on port 8000 with FastAPI/Uvicorn
# Routes are registered with prefixes: /telemetry, /policy, /forensics, /dpi, etc.
VITE_API_BASE_URL=http://127.0.0.1:8000
VITE_API_TIMEOUT=30000

# WebSocket
# WebSocket endpoint for real-time updates
VITE_WEBSOCKET_URL=ws://127.0.0.1:8000
VITE_WEBSOCKET_RECONNECT_INTERVAL=3000
VITE_WEBSOCKET_MAX_RETRIES=5

# Authentication
VITE_AUTH_TOKEN_KEY=jarvis_auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=jarvis_refresh_token
VITE_AUTH_SESSION_TIMEOUT=3600000
VITE_PQC_ENABLED=true

# Feature Flags
VITE_ENABLE_PASM=true
VITE_ENABLE_SELF_HEALING=true
VITE_ENABLE_CED=true
VITE_ENABLE_FORENSICS=true
VITE_ENABLE_3D_GLOBE=true
VITE_ENABLE_REALTIME_TELEMETRY=true

# Monitoring & Analytics
VITE_ENABLE_SENTRY=false
VITE_SENTRY_DSN=
VITE_SOURCEMAP=false

# CDN
VITE_CDN_URL=
VITE_HUAWEI_CDN_BUCKET=jarvis-assets

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
